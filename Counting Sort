int* countingSort(int arr_count, int* arr, int* result_count) 
{
    int u, i, j;
    int * a = malloc(sizeof(int) * 100);
    for (int j = 0; j < 100; j++)
    {
        a[j] = 0;
    }
     for(int i = 0; i < arr_count; i++)
     {
            u = arr[i];
            a[u]++;
     }
     *result_count = 100;
     return (a);
}
